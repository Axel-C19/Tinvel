{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/favicon.ico", "*.{css,scss,js,mjs,json,png,gif,ico,jpg,svg,woff,woff2,ttf,eot}"]
  },
  "mimeTypes": {
    ".js": "text/javascript",
    ".mjs": "text/javascript",
    ".json": "application/json",
    ".css": "text/css"
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' https: data: blob:;",
    "X-Content-Type-Options": "nosniff"
  },
  "routes": [
    {
      "route": "/*",
      "headers": {
        "cache-control": "no-cache"
      }
    }
  ]
}